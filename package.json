{
  "name": "vln-website",
  "version": "0.11.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest",
    "test:widget": "vitest __tests__/ZammadWidget.test.tsx",
    "test:widget:ui": "vitest __tests__/ZammadWidget.test.tsx --ui",
    "generate:favicons": "node scripts/generate-favicons.js && node scripts/generate-favicon-ico.js",
    "generate:icons": "pnpm generate:favicons",
    "docs:dev": "cd docs-site && pnpm dev",
    "docs:build": "cd docs-site && pnpm build",
    "docs:preview": "cd docs-site && pnpm preview",
    "docs:lint": "cd docs-site && pnpm lint",
    "design:dev": "cd design-site && pnpm dev",
    "design:build": "cd design-site && pnpm build",
    "design:preview": "cd design-site && pnpm preview",
    "design:lint": "cd design-site && pnpm lint",
    "monorepo:build": "pnpm -r build",
    "monorepo:lint": "pnpm -r lint",
    "monorepo:test": "pnpm --filter=vln-website test && pnpm --filter=vln-docs-site test || true"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.22",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.554.0",
    "next": "^16.1.5",
    "pcb-trace-animation": "^1.0.1",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.5.6",
    "sharp": "^0.34.5",
    "to-ico": "^1.1.5",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
